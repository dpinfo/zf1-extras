{
    "name": "dpinfo/zf1-extras",
    "description": "Zend Framework 1 Extras PHP 7.4 compatible",
    "type": "library",
    "homepage": "https://github.com/dpinfo/zf1-extras",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.4",
        "dpinfo/zf1": ">=1.12.22"
    },
    "autoload": {
        "psr-0": {
            "ZendX_": "library/"
        }
    },
    "include-path": [
        "library/"
    ],
    "replace": {
        "zendframework/zf1-extras": "1.*"
    }
}
